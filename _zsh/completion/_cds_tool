#compdef cds_tool

typeset -A opt_args
local context state line

_arguments "--feed+: :(us ca br uk de fr it es in jp cn)" \
  "--parallel+: :" \
  "--dataset+: :(Hits SortedHits SessionizedHits)" \
  "--time+[yyyy-mm-ddThh]:[yyyy-mm-ddThh]:" \
  "--service+: :(na eu fe)" \
  "(--head)--fields+: :(requestId session)" \
  "(--head)--filter+: :" \
  && return 0
